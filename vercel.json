{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    {
      "source": "/projects/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/projects/:path*",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}